{
  "name": "nomad-compass",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"bunx --bun vite\" \"wrangler dev\"",
    "dev:bun": "concurrently \"VITE_BACKEND=bun bunx --bun vite\" \"bun run src/entry.bun.dev.ts\"",
    "dev:vite": "bunx --bun vite",
    "dev:worker": "wrangler dev",
    "dev:api": "bun run src/entry.bun.dev.ts",
    "build": "bunx --bun vite build",
    "build:bun": "bun build src/entry.bun.ts --outdir=dist-bun --target=bun --minify",
    "build:all": "bun run build && bun run build:bun",
    "preview": "bunx --bun vite preview",
    "deploy:cf": "bun run build && wrangler deploy",
    "docker:build": "bun run build:all && docker build -t nomad-compass .",
    "lint": "bunx eslint src/",
    "typecheck": "bunx tsc --noEmit"
  },
  "dependencies": {
    "hono": "^4.11.3",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.9",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "wrangler": "^4.54.0"
  }
}

{
  "name": "nomad-compass",
  "version": "0.2.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"vite\" \"wrangler dev\"",
    "dev:node": "concurrently \"VITE_BACKEND=node vite\" \"npx tsx src/entry.node.dev.ts\"",
    "dev:vite": "vite",
    "dev:worker": "wrangler dev",
    "dev:api": "npx tsx src/entry.node.dev.ts",
    "build": "vite build",
    "build:node": "tsup",
    "build:all": "pnpm run build && pnpm run build:node",
    "preview": "vite preview",
    "deploy:cf": "pnpm run build && wrangler deploy",
    "docker:build": "pnpm run build:all && docker build -t nomad-compass .",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.7",
    "hono": "^4.11.3",
    "lucide-react": "^0.562.0",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-router-dom": "^7.11.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/node": "^25",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.23",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "wrangler": "^4.54.0"
  },
  "packageManager": "pnpm@10.27.0+sha512.72d699da16b1179c14ba9e64dc71c9a40988cbdc65c264cb0e489db7de917f20dcf4d64d8723625f2969ba52d4b7e2a1170682d9ac2a5dcaeaab732b7e16f04a"
}
